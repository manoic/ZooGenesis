						if refillPrice > cv.Value then 
							if refillPrice + fee > bv.Value then
								return false,"NOT ENOUGH BANK"
							else
								refillPrice += fee
								buyType = 2
							end
							--return false,"NOT ENOUGH CASH"
						end
					else
						if refillPrice > cv.Value then 
							return false,"NOT ENOUGH CASH"
						end
					end
					
