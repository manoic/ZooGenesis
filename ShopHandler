		local gottt = nil
		local got2

		local function gottChec(dir)
			if gottt then return end
			for _,v in pairs(dir:GetChildren()) do
				if (v:IsA("Tool") or v:IsA("Folder")) and v.Name == found.Name then
					got2 = v
					got = got + 1

					if not gottt then
						gottt = v
						break
					end
				end
			end		
		end

